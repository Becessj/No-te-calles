<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.notecalles" version="4.0.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="FileOpener2">
        <param name="android-package" value="io.github.pwlin.cordova.plugins.fileopener2.FileOpener2" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Â¡NO TE CALLES!</name>
    <description>
        VIOLENCIA
    </description>
    <author email="informatica@guamanpoma.org">
        Guaman Poma de Ayala
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="assets/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="assets/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="assets/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="assets/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="assets/mipmap-xxxhdpi/ic_launcher.png" />
    <splash density="land-hdpi" src="assets/drawable-land-hdpi/screen.png" />
    <splash density="land-ldpi" src="assets/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="assets/drawable-land-mdpi/screen.png" />
    <splash density="land-xhdpi" src="assets/drawable-land-xhdpi/screen.png" />
    <splash density="port-hdpi" src="assets/drawable-port-hdpi/screen.png" />
    <splash density="port-ldpi" src="assets/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="assets/drawable-port-mdpi/screen.png" />
    <splash density="port-xhdpi" src="assets/drawable-port-xhdpi/screen.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="android-minSdkVersion" value="22" />
</widget>
